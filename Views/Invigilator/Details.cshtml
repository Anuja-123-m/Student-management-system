@model StudentRecordManagementSystem.Models.Student
@{
    ViewData["Title"] = "Student Details";
}
<h2>Student Details</h2>
<div class="card">
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-3">Roll Number</dt>
            <dd class="col-sm-9">@Model.RollNumber</dd>
            <dt class="col-sm-3">Name</dt>
            <dd class="col-sm-9">@Model.Name</dd>
            <dt class="col-sm-3">Maths</dt>
            <dd class="col-sm-9">@Model.Maths</dd>
            <dt class="col-sm-3">Physics</dt>
            <dd class="col-sm-9">@Model.Physics</dd>
            <dt class="col-sm-3">Chemistry</dt>
            <dd class="col-sm-9">@Model.Chemistry</dd>
            <dt class="col-sm-3">English</dt>
            <dd class="col-sm-9">@Model.English</dd>
            <dt class="col-sm-3">Programming</dt>
            <dd class="col-sm-9">@Model.Programming</dd>
        </dl>
        <a class="btn btn-secondary" asp-action="Index">Back</a>
    </div>
</div>
